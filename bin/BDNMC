#!/bin/bash
set -e

START=$(date +%s)
if [! -d "/Events"]
then
	mkdir Events
fi

cd Source
if [! -e "main"]
then
	make main
fi
cd ..
if (($# != 0))
then
    for arg in "$@"
    do
        ./Source/main $arg
    done
else
    ./Source/main
fi

END=$(date +%s)
DIFF=$(( $END - $START ))
echo "Execution time = $DIFF s"
